<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<!-- Main.fxml -->

<?scenebuilder-stylesheet ./styles.css?>

<StackPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="1080.0"
	prefWidth="1920.0" xmlns="http://javafx.com/javafx/21"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="pl.edu.pw.mini.zpoif.project.binance.controllers.MainController">



	<children>
		<TabPane prefHeight="200.0" prefWidth="200.0"
			tabClosingPolicy="UNAVAILABLE">
			<tabs>
				<Tab text="Home Page">
					<content>
						<!-- Include the Home Page Tab FXML -->
						<fx:include fx:id="homepageTab" prefHeight="750.0"
							prefWidth="1500.0" source="homepage.fxml" />
					</content>
				</Tab>
				<Tab text="Przegląd rynków">
					<content>
						<!-- Include the Overview Tab FXML -->
						<fx:include fx:id="marketsreviewTab" prefHeight="750.0"
							prefWidth="1500.0" source="marketsreview.fxml" />
					</content>
				</Tab>
				<Tab text="Ulubione">
					<content>
						<!-- Include the Favorites Tab FXML -->
						<fx:include fx:id="favouriteTab" prefHeight="750.0"
							prefWidth="1500.0" source="favourite.fxml" />
					</content>
				</Tab>
				<Tab text="Analiza Trendów">
					<content>
						<fx:include fx:id="trendAnalysisTab" prefHeight="750.0"
							prefWidth="1500.0" source="trendanalysis.fxml" />
					</content>
				</Tab>

			</tabs>
		</TabPane>
	</children>

</StackPane>
